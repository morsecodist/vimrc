{
    "languageserver": {
        "terraform": {
            "command": "terraform-ls",
            "args": ["serve"],
            "filetypes": [
                "terraform",
                "tf"
            ],
            "initializationOptions": {},
            "settings": {}
        }
    },
    "rust-analyzer.rust-client.channel": "nightly",
    "diagnostic.virtualText": true,
    "python.formatting.provider": "flake8",
    "python.linting.flake8Enabled": true
}
